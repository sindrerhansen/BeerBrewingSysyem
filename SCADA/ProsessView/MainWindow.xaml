<Window x:Class="ProsessView.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:oxy="http://oxyplot.org/wpf"
        xmlns:local="clr-namespace:ProsessView"
        
        Title="MainWindow" Height="416.208" Width="825.417">

    <Grid>
        <oxy:Plot Title="{Binding Title}" Name="Plot">
            <oxy:Plot.Series>
                <oxy:LineSeries ItemsSource="{Binding Points}"/>
            </oxy:Plot.Series>
        </oxy:Plot>
        <Button x:Name="btnAddValue" Content="Button" HorizontalAlignment="Left" Margin="10,14,0,0" VerticalAlignment="Top" Width="75" Click="btnAddValue_Click"/>
    </Grid>
    
</Window>

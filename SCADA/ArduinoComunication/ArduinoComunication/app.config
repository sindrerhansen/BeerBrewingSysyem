<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <add key="ArduinoCom" value="COM9"/>
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <system.serviceModel>
    <services>
      <service name="ArduinoComunication.ArduinoComService" behaviorConfiguration="mexBehavior">
        <endpoint address="ArduinoComunicationService" binding="basicHttpBinding" contract="ArduinoComunication.IArduinoComService" ></endpoint>
        <endpoint address="ArduinoComunicationService" binding="netTcpBinding" contract="ArduinoComunication.IArduinoComService" ></endpoint>
        <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange"></endpoint>
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:9090/"/>
            <add baseAddress="net.tcp://localhost:9091/"/>
          </baseAddresses>
        </host>
      </service>

    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior name="mexBehavior">
          <serviceMetadata httpGetEnabled="true" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>